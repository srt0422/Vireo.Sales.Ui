build - ui - apptivator - dev {
  "name": "vireo.ui.apptivator",
  "version": "0.0.1",
  "description": "Generates cross platform mobile apps using the vireo.ui.apptivator.template and filling it with content in Content.js.",
  "main": "server.js",
  "engines": {
    "node": "7.7.x"
  },
  "scripts": {
    "start": "node server.js",

    "debug-server": "node inspect web/src/server/server.js",

    "start-server-dev": "cd web/src/server & node --inspect server.js",
    "start-ui-dev": "cross-env webpack-dev-server --config web/webpack.config.dev.js --colors --inline --hot",

    "build-all-smm-dev": "SET SITE_NAME=smm&& start npm run build-ui-dev&& start npm run build-server-dev&& start npm run start-server-dev&& start mongod --dbpath=/mongodata&& npm run start-ui-dev",
    "build-all-apptivator-dev": "SET SITE_NAME=mobile-app-builder&& start npm run build-ui-dev&& start npm run build-server-dev&& start npm run start-server-dev&& start mongod --dbpath=/mongodata&& npm run start-ui-dev",

    "build-ui-dev": "webpack --config web/webpack.config.dev.js --progress  --colors -w",
    "build-server-dev": "webpack --config web/webpack.config.server.dev.js --progress  --colors -w",

    "build-ui-smm-rc": "SET API_HOST=http://apptivator-rc-new.azurewebsites.net&& npm run build-ui-smm",
    "build-ui-apptivator-rc": "SET API_HOST=http://apptivator-rc-new.azurewebsites.net&& npm run build-ui-apptivator",

    "build-ui-smm": "del /q /s \"dist/\" \"web\\src\\client\\*.bundle\" \"web\\src\\client\\*.hot-update.js*\" & cross-env SITE_NAME=smm NODE_ENV=production webpack --config web/webpack.config.prod.js --progress & xcopy \"./web/src/client/.\" \"dist/client\" /I /Y /S & xcopy \"./web/src/server/.\" \"dist/server\" /I /Y /S & xcopy \"package.json\" \"dist/server\" /y",
    "build-ui-apptivator": "del /q /s \"dist/\" \"web\\src\\client\\*.bundle\" \"web\\src\\client\\*.hot-update.js*\" & cross-env SITE_NAME=mobile-app-builder NODE_ENV=production webpack --config web/webpack.config.prod.js --progress & xcopy \"./web/src/client/.\" \"dist/client\" /I /Y /S & xcopy \"./web/src/server/.\" \"dist/server\" /I /Y /S & xcopy \"package.json\" \"dist/server\" /y",
    "build-server-vireo_api": "del /q /s \"dist/\" \"web\\src\\client\\*.bundle\" \"web\\src\\client\\*.hot-update.js*\" & cross-env NODE_ENV=production webpack --config web/webpack.config.server.prod.js --progress & xcopy \"./web/src/client/.\" \"dist/client\" /I /Y /S & xcopy \"./web/src/server/.\" \"dist/server\" /I /Y /S & xcopy \"package.json\" \"dist/server\" /y",

    "test": "mocha --compilers js:babel-register --require babel-polyfill --recursive app/**/__tests__/*.js server/**/__tests__/*.js",
    "test:watch": "npm test -- --watch",
    "lint": "eslint app/**/*.js"
  },
  "keywords": [
    "react",
    "native",
    "nativebase",
    "web"
  ],
  "author": "Chion82",
  "license": "ISC",
  "dependencies": {
    "color": "^0.11.3",
    "exoskeleton": "^0.7.0",
    "handlebars": "^4.0.10",
    "hapi": "^16.5.0",
    "hapi-cors": "^1.0.1",
    "hapi-email-plugin": "^0.0.1",
    "hexo-server": "^0.2.2",
    "jsx-loader": "^0.13.2",
    "mongodb": "^2.2.30",
    "native-base-web": "0.0.4",
    "overjoy-await": "^0.0.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-native-web-extended": "0.0.7",
    "react-redux": "^4.4.5",
    "react-stripe-elements": "^0.0.6",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.11.1",
    "rewire": "^2.5.2",
    "sinon": "^2.4.1",
    "stripe": "^4.23.1",
    "then-yield": "^0.0.1",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "@kadira/storybook": "1.28.1",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class": "^0.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.14.0",
    "babel-runtime": "6.9.2",
    "chai": "^4.1.0",
    "cross-env": "^5.1.1",
    "del-cli": "^0.2.1",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "loglevel": "^1.4.1",
    "mocha": "^3.0.2",
    "node-todo-server": "0.0.1",
    "qrcode-terminal": "^0.10.0",
    "safe-buffer": "^5.1.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "2.6.0",
    "whatwg-fetch": "^2.0.3"
  }
}
