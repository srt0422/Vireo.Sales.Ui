{
  "name": "vireo.ui.apptivator",
  "version": "0.0.1",
  "description": "Generates cross platform mobile apps using the vireo.ui.apptivator.template and filling it with content in Content.js.",
  "main": "server.js",
  "engines": { "node": "7.7.x" },
  "scripts": {
    "start": "node server.js",
    "backend": "node web/src/server/server.js",
    "server-debug": "node inspect web/src/server/server.js",
    "dev": "webpack-dev-server --config web/webpack.config.dev.js --colors",
    "build-dev": "yarn run clean & webpack --config web/webpack.config.dev.js --progress  --colors --hot -w",
    "build": "del /q /s \"dist/\" & del /q \"web/src/client/.\" & cross-env NODE_ENV=production API_HOST=http://apptivator.azurewebsites.net/api webpack --config web/webpack.config.prod.js --progress && xcopy \"./web/src/client/.\" \"dist/client/.\" /Y /S && xcopy \"./web/src/server/.\" \"dist/server/.\" /Y /S && copy \"package.json\" \"dist/server/\" /y",
    "clean": "del \"web\\src\\*.js\"",
    "test": "mocha --compilers js:babel-register --require babel-polyfill --recursive app/**/__tests__/*.js server/**/__tests__/*.js",
    "test:watch": "npm test -- --watch",
    "lint": "eslint app/**/*.js"
  },
  "keywords": [
    "react",
    "native",
    "nativebase",
    "web"
  ],
  "author": "Chion82",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-transform-class": "^0.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "color": "^0.11.3",
    "cross-env": "^5.0.1",
    "exoskeleton": "^0.7.0",
    "hapi": "^16.5.0",
    "hapi-cors": "^1.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jsx-loader": "^0.13.2",
    "mongodb": "^2.2.30",
    "native-base-web": "0.0.4",
    "overjoy-await": "^0.0.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-native-web-extended": "0.0.7",
    "react-redux": "^4.4.5",
    "react-stripe-elements": "^0.0.6",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.11.1",
    "rewire": "^2.5.2",
    "sinon": "^2.4.1",
    "stripe": "^4.23.1",
    "then-yield": "^0.0.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "1.28.1",
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.14.0",
    "babel-runtime": "6.9.2",
    "chai": "^4.1.0",
    "del-cli": "^0.2.1",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "expect": "^1.20.2",
    "file-loader": "^0.11.2",
    "loglevel": "^1.4.1",
    "mocha": "^3.0.2",
    "my-ip": "^1.1.1",
    "node-todo-server": "0.0.1",
    "qrcode-terminal": "^0.10.0",
    "url-loader": "^0.5.9",
    "webpack": "3.4.0",
    "webpack-dev-server": "2.6.0"
  }
}
